

<div class=page-header>
<div class=pull-right>
  <%= link_to 'New User', new_user_path, class:'btn btn-info' %>
</div>
<h1>Users</h1>
</div>


<table class="table">
  <thead>
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th colspan='3'></th>
  </tr>
</thead>
  <tbody>
<% @users.each do |user| %>
<tr>
  <td><%= user.fullname%></td>
  <td><%= mail_to user.email %></td>
</tr>
<% end %>
</table>
